{
    "name": "replayer",
    "version": "1.4.3",
    "description": "Replayer is a free, cue-based media player for rehearsals with playback music. By the click of a button, Replayer starts to play at predefined times in the audio file.",
    "homepage": "https://replayer.app",
    "main": "./public/index.html",
    "private": true,
    "keywords": [
        "mp3, wav, ogg, flac, aiff, aac, player, start, point, time, button, click, link, music, file, voice, choir, singer, solo, song, software, app, free, fast, simple, Spotify, YouTube, Vimeo, SoundCloud, SATB, Soprano, Alto, Tenor, Bass, drum, track, entertainer, dancer"
    ],
    "author": {
        "name": "Marcel Suter",
        "email": "replayer.app@marcelsuter.ch",
        "url": "https://marcelsuter.ch"
    },
    "license": "GPL-3.0-or-later",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "css-build": "sass --no-source-map sass/replayer.scss public/css/replayer.css",
        "css-watch": "npm run css-build -- --watch",
        "start": "npm run css-watch",
        "test": "jest --ci --reporters=default --reporters=jest-junit -- --watch",
        "cypress:open": "cypress open",
        "cypress:run": "npm run serve & wait-on http://localhost:8080 & npx cypress run",
        "cypress:run-chrome": "npm run serve & wait-on http://localhost:8080 & npx cypress run --browser chrome",
        "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json"
    },
